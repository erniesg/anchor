# Cloudflare Account
CLOUDFLARE_ACCOUNT_ID="your-account-id-here"
CLOUDFLARE_API_TOKEN="your-api-token-here"

# Environment (dev or prod)
ENVIRONMENT="dev"

# Database Names
DB_NAME_DEV="anchor-dev-db"
DB_NAME_PROD="anchor-prod-db"

# R2 Bucket Names
R2_BUCKET_DEV="anchor-dev-storage"
R2_BUCKET_PROD="anchor-prod-storage"

# Worker Names
WORKER_NAME_DEV="anchor-dev-api"
WORKER_NAME_PROD="anchor-prod-api"

# Pages Project Name
PAGES_PROJECT_NAME="anchor"

# Auth (Logto)
LOGTO_ENDPOINT="https://your-logto-domain.com"
LOGTO_APP_ID="your-app-id"
LOGTO_APP_SECRET="your-app-secret"

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET="generate-random-string-here"

# Feature Flags
ENABLE_DEBUG_LOGS="true"
ENABLE_ANALYTICS="false"